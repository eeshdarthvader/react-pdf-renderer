{
    "name": "@react-pdf-renderer/download",
    "version": "0.0.1",
    "description": "A React component to view a PDF document",
    "keywords": [
        "react",
        "react.js",
        "pdf",
        "pdf.js",
        "pdf viewer"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/eeshdarthvader/react-pdf-renderer",
        "directory": "packages/download"
    },
    "bugs": {
        "url": "https://github.com/eeshdarthvader/react-pdf-renderer/issues"
    },
    "main": "npm/index.js",
    "types": "npm/index.d.ts",
    "files": [
        "npm/styles.css",
        "npm/cjs/*.js",
        "npm/index.d.ts",
        "npm/index.js"
    ],
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "@react-pdf-renderer/core": "^2.0.0"
    },
    "scripts": {
        "clean": "rm -rf npm && mkdir npm",
        "build": "npm run clean && cp src/index.d.ts npm && cp dist/index.js npm && NO_CSS='true' rollup -c ../../rollup.config.js"
    }
}